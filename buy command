//      command fishing rod      //

Command trigger : <prefix>buy fishing rod
Variables : fishing rod, money


$nomention
$onlyIf[$getUserVar[money]>=1250; You do not have enough money!]
$title[successful purchase]
$color[ff00be]
$description[$username you bought a fishing rod for 1250$ !] 
$setUserVar[money;$sub[$getUserVar[money];1250]]
$setUserVar[fishing rod;$sum[$getUserVar[fishing rod];1]]

